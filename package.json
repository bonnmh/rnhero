{
  "name": "TET",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "pod": "cd ios && pod install && cd ..",
    "installLib": "yarn add ",
    "installPod": "func() { npm run installLib \"$1\" && npm run pod; }; func",
    "yp": "npm run installPod -- ",
    "android-apk": "yarn bundle-js-android && cd android && ./gradlew assembleRelease",
    "bundle-js-android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle",
    "push-android": "appcenter codepush release-react -a bonnmh/TET -d Staging",
    "android-sh1": "cd android && ./gradlew signingReport"
  },
